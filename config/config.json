{
  "api": {
    "jwt_secret": "${JWT_SECRET:-5up3r5tr0n6p455}"
  },
  "app": {
    "mode": "${APP_MODE:-release}",
    "port": "${APP_PORT:-5000}",
    "env_files": []
  },
  "org": {
    "abbr": "XI",
    "name": "Xet Industries",
    "domain": "xetindustries.com",
    "url": "https://xetindustries.com",
    "img": [
      "public/res/static/org/icon-wordmark-black-bg-2000x1500.svg"
    ],
    "logo": []
  },
  "store": {
    "db": {
      "default_profile": "${DB_DEFAULT:-db}",
      "conn": {
        "db": {
          "enable": "${DB_ENABLE:-true}",
          "db": "${DB:-XI}",
          "user": "${DB_USER:-XI_u}",
          "pass": "${DB_PASS}",
          "driver": "${DB_DRIVER:-mysql}",
          "host": "${DB_HOST:-127.0.0.1}",
          "port": "${DB_PORT:-3306}",
          "charset": "${DB_CHARSET:-utf8mb4}",
          "engine": "${DB_ENGINE}",
          "collation": "${DB_COLLATION:-utf8mb4_unicode_ci}",
          "socket": "${DB_SOCKET}",
          "prefix": "${DB_PREFIX}",
          "prefixindexes": true,
          "strict": true
        }
      }
    },
    "rdb": {
      "default_profile": "${RDB_DEFAULT:-rdb}",
      "conn": {
        "rdb": {
          "enable": "${RDB_ENABLE:-true}",
          "rdb": "${RDB:-0}.int",
          "driver": "redis",
          "host": "${RDB_HOST:-127.0.0.1}",
          "port": "${RDB_PORT:-6379}",
          "pass": ""
        }
      }
    }
  },
  "web": {
    "default": {
      "org": {
        "name": "Xet Industries",
        "domain": "xetindustries.com",
        "url": "https://xetindustries.com",
        "img": {
          "url": "/res/static/org/icon-wordmark-black-bg-2000x1500.svg"
        }
      },
      "meta": {
        "robots": "index, follow",
        "referrer": "no-referrer-when-downgrade",
        "tags": [
          "xetindustries",
          "xet industries",
          "Xtreme Embedded Tech Industries"
        ]
      },
      "nav_menu": [
        {
          "label": "Home",
          "url": "/"
        },
        {
          "label": "Blog",
          "url": "/blog"
        },
        {
          "label": "Product",
          "url": "#"
        },
        {
          "label": "Support",
          "url": "#"
        },
        {
          "label": "Contact",
          "url": "#"
        }
      ],
      "css": [
        "/res/css/app.css"
      ],
      "js99": [
        "/res/js/app.js",
        "/res/js/util/content-src.js"
      ]
    },
    "pages": {
      "home": {
        "route": "/",
        "ctrl": {
          "render": "file"
        },
        "content": {
          "file": "view/partials/home/home.part.html"
        },
        "meta": {
          "title": "Xet Industries",
          "description": "A Collaborative Platform for Makers, Creators, and Developers."
        },
        "web": {
          "menu": "Home"
        }
      },
      "blog": {
        "route": "/blog",
        "ctrl": {
          "render": "file"
        },
        "content": {
          "file": "view/partials/blog/blog.part.html"
        },
        "meta": {
          "title": "Blog",
          "description": "Discover Insightful Articles, Expert Tips and Inspiring Ideas. Write, Share and Connect with a thriving like-minded community.",
          "canonical": "https://xetindustries.com/blog"
        },
        "web": {
          "menu": "Blog",
          "sub_brand": "Blog"
        },
        "css": [
          "/res/css/app.css",
          "/res/css/blog.css"
        ],
        "js99": [
          "/res/js/app.js",
          "/res/js/util/content-src.js",
          "/res/js/util/navigateByDataHref.js",
          "/res/js/blog.js"
        ]
      },
      "blogs": {
        "route": "/blog/*",
        "ctrl": {
          "mode": "unmanaged",
          "layout": "layout/blogs"
        },
        "web": {
          "menu": "Blog",
          "sub_brand": "Blog"
        },
        "css": [
          "/res/css/app.css",
          "/res/css/blogs.css"
        ],
        "js99": [
          "/res/js/app.js",
          "/res/js/util/content-src.js",
          "/res/js/util/code-copy.js",
          "/res/js/util/clickable-h2.js"
        ],
        "lib99": [
          "hljs"
        ]
      }
    }
  }
}